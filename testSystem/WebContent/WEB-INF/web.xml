<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <context-param>
    <param-name>driver_class</param-name>
    <param-value>com.mysql.jdbc.Driver</param-value>
  </context-param>
  <context-param>
    <param-name>url</param-name>
    <param-value>jdbc:mysql://localhost:3306/test</param-value>
  </context-param>
  <context-param>
    <param-name>username</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>password</param-name>
    <param-value>mrtimlong</param-value>
  </context-param>
  
  
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>cn.edu.nuist.testSystem.controller.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MyLoginServlet</servlet-name>
    <servlet-class>cn.edu.nuist.testSystem.controller.MyLoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>QuestionInputServlet</servlet-name>
    <servlet-class>cn.edu.nuist.testSystem.controller.QuestionInputServlet</servlet-class>
  </servlet>
  <servlet>
