    <servlet-class>cn.edu.nuist.testSystem.controller.ImportQuesServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ViewChoicesQuestionsServlet</servlet-name>
    <servlet-class>cn.edu.nuist.testSystem.controller.ViewChoicesQuestionsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ViewGap_fillQuestionsServlet</servlet-name>
    <servlet-class>cn.edu.nuist.testSystem.controller.ViewGap_fillQuestionsServlet</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>ProducePaperServlet</servlet-name>
    <servlet-class>cn.edu.nuist.testSystem.controller.ProducePaperServlet</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyLoginServlet</servlet-name>
    <url-pattern>/MyLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QuestionInputServlet</servlet-name>
    <url-pattern>/QuestionInputServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ViewQuestionsServlet</servlet-name>
    <url-pattern>/ViewQuestionsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ImportQuesServlet</servlet-name>
    <url-pattern>/ImportQuesServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ViewChoicesQuestionsServlet</servlet-name>
    <url-pattern>/ViewChoicesQuestionsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ViewGap_fillQuestionsServlet</servlet-name>
    <url-pattern>/ViewGap_fillQuestionsServlet</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>ProducePaperServlet</servlet-name>
    <url-pattern>/ProducePaperServlet</url-pattern>
  </servlet-mapping>
  
</web-app>